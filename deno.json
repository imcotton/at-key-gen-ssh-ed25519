{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@key/gen-ssh-ed25519",
  "version": "0.3.2",
  "lock": false,
  "imports": {
    "ed25519-keygen/ssh": "npm:ed25519-keygen@~0.4.11/ssh"
  },
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noUncheckedIndexedAccess": true,
    "strict": true
  },
  "publish": {
    "include": [
      "deno.json",
      "package.json",
      "LICENSE.txt",
      "README.md",
      "./src"
    ],
    "exclude": [
      "node_modules/",
      "tests",
      ".git/",
      ".vscode/",
      ".github/"
    ]
   },
  "exports": {
    "./private": "./src/private.ts",
    "./uuid": "./src/uuid.ts",
    "./pub": "./src/pub.ts",
    ".": "./src/mod.ts"
  }
}
