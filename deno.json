{
  "$schema": "https://raw.esm.sh/gh/denoland/deno@v2.4.4/cli/schemas/config-file.v1.json",
  "name": "@key/gen-ssh-ed25519",
  "version": "0.8.0",
  "lock": false,
  "imports": {
    "@std/assert":     "jsr:@std/assert@1",
    "@std/encoding": "jsr:@std/encoding@1",
    "@std/testing":   "jsr:@std/testing@1",
    "micro-key-producer/ssh.js": "jsr:@paulmillr/micro-key-producer@~0.7.3/ssh.js"
  },
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "strict": true
  },
  "publish": {
    "include": [
      "deno.json",
      "package.json",
      "LICENSE.txt",
      "README.md",
      "./tests",
      "./src"
    ],
    "exclude": [
      "node_modules/",
      ".git/",
      ".vscode/",
      ".github/"
    ]
   },
  "exports": {
    "./private": "./src/private.ts",
    "./uuid": "./src/uuid.ts",
    "./main": "./src/main.ts",
    "./pub": "./src/pub.ts",
    ".": "./src/mod.ts"
  }
}
